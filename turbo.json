{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "develop": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "test:ci": {
      "cache": false
    },
    "test:watch": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
